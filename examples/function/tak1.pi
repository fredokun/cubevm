
def Tak(x,y,z,r) =
                 if y >= x then r!(z)
                 else new(r1),new(r2),new(r3),
           [ Tak(x-1,y,z,r1) || Tak(y-1,x,z,r2) || Tak(z-1,x,y,r3) ||
             r1?(nx),r2?(ny),r3?(nz),Tak(nx,ny,nz,r) ];

new(r), [ Tak(96,48,32,r) || r?(res),#println("Result is = "+res) ]

